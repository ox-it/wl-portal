	<dependencies>
        <classpathentry kind="var" path="MAVEN_REPO/commons-logging/jars/commons-logging-1.0.4.jar"/>
        <classpathentry combineaccessrules="false" kind="src" path="/000portal-api"/>
        <classpathentry combineaccessrules="false" kind="src" path="/component-api"/>
