#start_response()
<!-- title.vm -->
<div class="portletTitle">
	<div class="title">
#if ($showResetButton)
		<a href="${resetActionUrl} title="Reset">
			<img src="/library/image/transparent.gif" alt="Reset" border="1" />
		</a>
#end
		<h2>$toolTitle</h2>
	</div>
	<div class="action">
#if ($showHelpButton)
		<a accesskey="h" href="${helpActionUrl} target="_blank" onclick="openWindow('$helpActionUrl', 'Help', 'resizable=yes,toolbar=no,scrollbars=yes,menubar=yes,width=800,height=600'); return false">
			<img src="/library/image/transparent.gif" alt="Help" border="0" />
		</a>
	</div>
</div>
#if ($resetToolNow)
<script type="text/javascript" language="JavaScript">
try
{
	if (parent.${mainFrameId}.location.toString().length > 1)
	{
		parent.${mainFrameId}.location = '${mainFrameUrl}';
	}
}
catch (e1)
{
	try
	{
		if (parent.parent.${mainFrameId}.location.toString().length > 1)
		{
			parent.parent.${mainFrameId}.location = '${mainFrameUrl}';
		}
	}
	catch (e2)
	{
	}
}
</script>
#end
<!-- end title.vm -->
#end_response()